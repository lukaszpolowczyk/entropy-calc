{
    "rules": {
        "semi": [2, "always"],
		semi-spacing: 2,
        "quotes": 0,
		"comma-dangle": 0,
		no-trailing-spaces: [0, { "skipBlankLines": true }],
		"space-infix-ops": 0,
		"comma-spacing": 2,
		"guard-for-in": 2,
		"no-alert": 2,
		"no-eq-null": 2,
		eqeqeq: 2,
		"strict": [2, "function"],
		"max-nested-callbacks": [2, 3],
		"complexity": 0,
		no-nested-ternary: 2,
		"max-depth": [2, 10],
		no-inner-declarations: 2,
		no-lone-blocks: 2,
		camelcase: 2,
		"no-mixed-spaces-and-tabs": [2],
		comma-style: [2, "last", {exceptions: {ArrayExpression: false, ObjectExpression: false} }],
		indent: [2, "tab", {"SwitchCase": 1}],
		no-use-before-define: 2,
		no-unused-vars: 2,
		no-undef: 2,
		no-new: 0,
		no-bitwise: 2,
		no-undef-init: 2,
		no-shadow-restricted-names: 2,
		"block-spacing": [2, "always"],
		operator-linebreak: [2, "before"],
		space-unary-ops: 2,
		"computed-property-spacing": [2, "never"],
		no-plusplus: 0,
		"max-len": [2, 260, 4, {"ignoreComments": true, "ignoreUrls": true, "ignorePattern": "^\\s*var\\s.+=\\s*require\\s*\\("}],
		init-declarations: 0,
		no-label-var: 2,
		no-undefined: 0,
		func-names: 0,
		no-spaced-func: 2,
		one-var: [2, "never"],
		"brace-style": [2, "1tbs", { "allowSingleLine": true }],
		no-lonely-if: 2,
		no-else-return: 2,
		space-before-blocks: 0,
		space-before-function-paren: [0, { "anonymous": "never", "named": "always" }],
		space-before-keywords: [2, "always"],
		space-return-throw-case: 2,
		"object-curly-spacing": [2, "always", {
			"objectsInObjects": true,
			"arraysInObjects": true
		}],
		key-spacing: [2, {"beforeColon": false, "afterColon": true}],
		space-in-parens: 0,
		spaced-comment: 0,
		no-multiple-empty-lines: [2, {max: 5}],
		dot-location: [2, "property"],
		lines-around-comment: 0,
		linebreak-style: 0,
		newline-after-var: 0,
		no-inline-comments: 0,
		no-multi-str: 0,
		no-unexpected-multiline: 2,
		no-extra-parens: 2,
		consistent-this: 0,
		id-length: 0,
		id-match: 0,
		new-cap: 2,
		no-array-constructor: 2,
		no-continue: 2,
		no-new-object: 2,
		no-restricted-syntax: 0,
		no-ternary: 2,
		no-underscore-dangle: 0,
		no-unneeded-ternary: 2,
		operator-assignment: [2, "always"],
		padded-blocks: 0,
		quote-props: [2, "as-needed"],
		require-jsdoc: 0,
		sort-vars: 2,
		wrap-regex: 2,
		no-caller: 2,
		consistent-return: 2,
		curly: 2,
		no-native-reassign: 2,
		vars-on-top: 0,
		no-inner-declarations: 2,
		no-redeclare: 2,
		default-case: 2,
		dot-notation: 2,
		no-div-regex: 2,
		no-empty-label: 2,
		no-eval: 2,
		no-extend-native: 2,
		no-extra-bind: 2,
		no-fallthrough: 2,
		no-floating-decimal: 2,
		no-implicit-coercion: 2,
		no-implied-eval: 2,
		no-invalid-this: 2,
		no-iterator: 2,
		accessor-pairs: 2,
		block-scoped-var: 2,
		yoda: 0,
		wrap-iife: 2,
		radix: 2,
		no-with: 2,
		no-void: 2,
		no-useless-concat: 0,
		no-useless-call: 2,
		no-unused-expressions: 2,
		no-return-assign: 2,
		no-script-url: 2,
		no-self-compare: 2,
		no-sequences: 2,
		no-throw-literal: 0,
		no-useless-concat: 0,
		no-proto: 2,
		no-process-env: 0,
		no-param-reassign: 2,
		no-octal: 2,
		no-octal-escape: 2,
		no-new-wrappers: 2,
		no-new-func: 2,
		no-multi-spaces: 2,
		no-loop-func: 0,
		no-labels: 2,
		space-after-keywords: 2,
		
		max-statements: 0,
		max-params: 0,
		no-extra-semi: 2,
		no-dupe-args: 2,
		arrow-parens: [0, "as-needed"],
		new-parens: 2,
		no-delete-var: 2,
		no-shadow: 2,
		func-style: 0,
		
		"no-console": 0,
		no-catch-shadow: 2,
    },
	"env": {
		"browser": true,
		"amd": true,
		"es6": true,
		"node": true
	},
	"ecmaFeatures": {
		"arrowFunctions": true,
		"forOf": true,
		"jsx": true,
		"destructuring": true,
		generators: true,
		templateStrings: true,
	}
}